{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "img/**/*-converted-to.pdf": true,
        "**/*.aux": true,
        "**/_minted-*": true,
        "**/*.lof": true,
        "**/*.log": true,
        "**/*.lot": true,
        "**/*.fls": true,
        "**/*.out": true,
        "**/*.toc": true,
        "**/*.fmt": true,
        "**/*.fot": true,
        "**/*.cb": true,
        "**/*.cb2": true,
        "**/.*.lb": true,
        "**/*.bbl": true,
        "**/*.bcf": true,
        "**/*.blg": true,
        "**/*-blx.aux": true,
        "**/*-blx.bib": true,
        "**/*.run.xml": true,
        "**/*.fdb_latexmk": true,
        "**/*.synctex": true,
        "**/*.synctex(busy)": true,
        "**/*.synctex.gz": true,
        "**/*.synctex.gz(busy)": true,
        "**/*.pdfsync": true,
        "**/*.alg": true,
        "**/*.loa": true,
        "**/*.lol": true,
        "**/*.pyg": true,
        "**/graphs/**/*.pdf": true
    },
    "editor.wordWrap": "on"
